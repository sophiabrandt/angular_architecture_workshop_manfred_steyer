{
  "projectType": "library",
  "sourceRoot": "libs/luggage/ui-card/src",
  "prefix": "flight-workspace",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/luggage/ui-card"],
      "options": {
        "jestConfig": "libs/luggage/ui-card/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/luggage/ui-card/**/*.ts",
          "libs/luggage/ui-card/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:luggage", "type:ui"]
}
